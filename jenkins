pipeline {
	agent none
	 stages {
	    stage('MAIN') {
	        parallel {
                   stage('build') {
		    agent { label 'master' }
		    steps { 
		      git branch: 'main', url: 'https://github.com/vijaybc98/c-project.git'
		       }
		   }
 	     stage('deploy') {
	         agent { label 'master' }
	         steps {
                     echo "this is a deploy stage"
	               }
	           }
             stage('testing') {
	          agent { label 'slave1' }
                  steps {
                       sh 'echo "this is a testing stage"'
                       }
                   }
		}
              }
	   }
	}
